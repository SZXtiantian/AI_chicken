@charset "UTF-8";
/* 水平间距 */
/* 水平间距 */
.uni-list-cell {
  justify-content: flex-start;
}
.uni-btn-v {
  padding: 10rpx 0;
}
.uni-btn-v button {
  margin: 20rpx 0;
  margin-left: 70rpx;
  margin-right: 70rpx;
  border-radius: 50rpx;
}
.uni-list {
  background-color: #FFFFFF;
  position: relative;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.uni-list-cell {
  position: relative;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.uni-list-cell-left {
  white-space: nowrap;
  font-size: 35rpx;
  padding: 0 30rpx;
}
.uni-input {
  height: 50rpx;
  padding: 15rpx 25rpx;
  line-height: 50rpx;
  font-size: 35rpx;
  background: #FFF;
  flex: 1;
}
.uni-input2 {
  height: 50rpx;
  padding: 15rpx 3rpx;
  line-height: 50rpx;
  font-size: 35rpx;
  background: #FFF;
  flex: 1;
}
.uni-input3 {
  height: 40rpx;
  padding: 15rpx 0;
  line-height: 50rpx;
  font-size: 35rpx;
  background: #FFF;
  flex: 1;
}
.uni-list-cell-db,
.uni-list-cell-right {
  flex: 1;
}
.uni-title {
  font-size: 40rpx;
  font-weight: 500;
  padding: 20rpx 0;
  line-height: 1.5;
}
.uni-common-pl {
  padding-left: 30rpx;
}

